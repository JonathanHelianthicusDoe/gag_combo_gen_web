{
    "printWidth": 79,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": false,
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "semi": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "arrowParens": "avoid",
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
